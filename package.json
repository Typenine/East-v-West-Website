{
  "name": "east-v-west",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/migrate-on-build.mjs && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "node scripts/db-migrate.mjs",
    "db:seed": "node scripts/db-seed.mjs",
    "migrate:blob-to-db": "tsx scripts/migrate-blob-to-db.ts",
    "migrate:blob-media-to-r2": "tsx scripts/migrate-blob-media-to-r2.ts",
    "cleanup:blob": "tsx scripts/cleanup-blob.ts",
    "setup:all": "npm run db:generate && npm run db:migrate && npm run db:seed && npm run migrate:blob-to-db -- --write && npm run migrate:blob-media-to-r2 -- --write && npm run cleanup:blob -- --confirm",
    "verify:r2": "node scripts/verify-r2.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.651.1",
    "@aws-sdk/s3-request-presigner": "^3.651.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.10.3",
    "@vercel/kv": "^1.0.1",
    "@xyflow/react": "12.8.3",
    "classnames": "2.5.1",
    "d3-dag": "1.1.0",
    "drizzle-orm": "0.36.0",
    "elkjs": "0.10.0",
    "fast-xml-parser": "^4.4.1",
    "html-to-image": "1.11.13",
    "jspdf": "3.0.1",
    "next": "15.4.10",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "svg2pdf.js": "2.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "0.26.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "overrides": {
    "use-sync-external-store": "1.5.0"
  }
}
